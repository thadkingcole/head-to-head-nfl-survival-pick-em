<h2>STANDINGS</h2>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Wins</th>
      <th scope="col">Point Diff</th>
    </tr>
  </thead>
  <tbody>
    {{#each allUsers}}
      <tr>
        <td>{{this.dataValues.username}}</td>
        <td>{{this.dataValues.wins}}</td>
        <td>{{this.dataValues.pointDiff}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>